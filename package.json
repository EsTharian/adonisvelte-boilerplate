{
  "name": "frontalis",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "eval $(grep '^HTTP_PORT' .env) && node ace serve --watch --encore-args=\"--host 0.0.0.0 --port ${HTTP_PORT}\"",
    "build": "node ace build --production",
    "start": "node server.js",
    "prod": "cp .env build/.env && cd build && yarn install --development && yarn start",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.0.0",
    "@symfony/webpack-encore": "^1.6.1",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "markdown-it": "^12.2.0",
    "pino-pretty": "^7.0.1",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "prismjs": "^1.25.0",
    "sass": "^1.42.0",
    "sass-loader": "^12.1.0",
    "svelte": "^3.43.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.15",
    "typescript": "~4.2",
    "uuid": "^8.3.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/ally": "^4.1.1",
    "@adonisjs/auth": "^8.0.9",
    "@adonisjs/core": "^5.3.0",
    "@adonisjs/lucid": "^16.0.2",
    "@adonisjs/redis": "^7.0.9",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.0.0",
    "@adonisjs/shield": "^7.0.0",
    "@adonisjs/view": "^6.0.0",
    "@eidellev/inertia-adonisjs": "^4.2.1",
    "@inertiajs/inertia": "^0.10.1",
    "@inertiajs/inertia-svelte": "^0.7.4",
    "@tailwindcss/line-clamp": "^0.2.2",
    "axios": "^0.21.4",
    "luxon": "^2.0.2",
    "pg": "^8.7.1",
    "phc-bcrypt": "^1.0.7",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.20"
  }
}
